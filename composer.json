{
    "name": "ayou/laravel-multichain",
    "description": "Laravel 5 wrapper for the Mulitchain ",
    "license": "MIT",
	"keywords": ["laravel", "multichain", "blockchain", "api"],
	"authors": [
        {
            "name": "Ayou xyz",
            "email": "abc@gmail.com"
        }
    ],
    "minimum-stability": "dev",
